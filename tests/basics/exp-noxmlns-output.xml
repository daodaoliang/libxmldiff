<?xml version="1.0" encoding="iso-8859-1" standalone="yes"?>
<tests status="below">
  <test name="Element identique vide"/>
  <test name="Element identique valeur">Identique</test>
  <test name="Element identique composé">
    <contenu>
      <composé>Identique</composé>
    </contenu>
  </test>
  <test name="Element ajouté vide" status="below">
    <element status="added"/>
  </test>
  <test name="Element ajouté debut" status="below">
    <element id="1" status="added"/>
    <element id="2"/>
    <element id="3"/>
    <element id="4"/>
  </test>
  <test name="Element ajouté milieu" status="below">
    <element id="2"/>
    <element id="1" status="added"/>
    <element id="3"/>
    <element id="4"/>
  </test>
  <test name="Element ajouté fin" status="below">
    <element id="2"/>
    <element id="3"/>
    <element id="4"/>
    <element id="5" status="added"/>
  </test>
  <test name="Element supprimé vide" status="below">
    <element id="1" status="removed"/>
  </test>
  <test name="Element supprimé début" status="below">
    <element id="1" status="removed"/>
    <element id="2"/>
    <element id="3"/>
    <element id="4"/>
    <element id="5"/>
  </test>
  <test name="Element supprimé milieu" status="below">
    <element id="1"/>
    <element id="2"/>
    <element id="3" status="removed"/>
    <element id="4"/>
    <element id="5"/>
  </test>
  <test name="Element supprimé fin" status="below">
    <element id="1"/>
    <element id="2"/>
    <element id="3"/>
    <element id="4"/>
    <element id="5" status="removed"/>
  </test>
  <test name="Element ajouté sans ordre" status="below">
    <element>1</element>
    <element status="modified">3|2</element>
    <element status="modified">4|3</element>
    <element status="modified">5|4</element>
    <element status="added">5</element>
  </test>
  <test name="Element supprimé sans ordre" status="below">
    <element>1</element>
    <element>2</element>
    <element status="modified">3|4</element>
    <element status="modified">4|5</element>
    <element status="removed">5</element>
  </test>
  <test name="Ajout elements variés" status="modified">|<![CDATA[ Element CData ]]><?element Processing instruction ?><!-- Commentaire -->
  Texte avec &lt; &gt; caractères spéciaux &amp;
 </test>
  <test name="Suppression elements variés" status="modified">
  Texte avec &lt; &gt; caractères spéciaux &amp;
 |<![CDATA[ Element CData ]]><!-- Commentaire --><?element Processing instruction ?></test>
  <test name="Elements variés identiques"><![CDATA[ Element CData ]]><?element Processing instruction ?><!-- Commentaire -->
  Texte avec &lt; &gt; caractères spéciaux &amp;
 </test>
  <test name="Elements variés modifiés" status="modified">
  Texte avec &lt; &gt; caractères spéciaux &amp; avant
 |<![CDATA[ Element CData - après ]]><?element Processing instruction - après ?><!-- Commentaire - après -->
  Texte avec &lt; &gt; caractères spéciaux &amp; après
 <![CDATA[ Element CData - avant ]]><!-- Commentaire - avant --><?element Processing instruction - avant ?></test>
  <test name="Ajout d'un fils unique" status="below">
    <root status="below">
      <child status="below">
        <element status="added"/>
      </child>
    </root>
  </test>
  <test name="Suppression d'un fils unique" status="below">
    <root status="below">
      <child status="below">
        <element status="removed"/>
      </child>
    </root>
  </test>
  <test name="Modification de texte" status="modified">Texte avant|Texte après</test>
  <test name="Texte variant suivant retour chariots entre balises" status="modified">
 Texte
 
 Texte
 
 | Texte <balise/> Texte <balise status="modified">
 Balise
 | Balise </balise> </test>
  <test name="Attribut identique mais inversés et espacés">
    <element att1="val1" att2="val2" att3="val3"/>
  </test>
  <test name="Ajout attribut seul" status="below">
    <element att="|val" status="modified"/>
  </test>
  <test name="Ajout attribut debut" status="below">
    <element att1="|val1" att2="val2" att3="val3" status="modified"/>
  </test>
  <test name="Ajout attribut milieu" status="below">
    <element att1="val1" att2="|val2" att3="val3" status="modified"/>
  </test>
  <test name="Ajout attribut fin" status="below">
    <element att1="val1" att2="val2" att3="|val3" status="modified"/>
  </test>
  <test name="Suppression attribut seul" status="below">
    <element attribut="valeur|" status="modified"/>
  </test>
  <test name="Suppression attribut début" status="below">
    <element att2="val2" att3="val3" att1="val1|" status="modified"/>
  </test>
  <test name="Suppression attribut milieu" status="below">
    <element att1="val1" att3="val3" att2="val2|" status="modified"/>
  </test>
  <test name="Suppression attribut fin" status="below">
    <element att1="val1" att2="val2" att3="val3|" status="modified"/>
  </test>
  <test name="Modification attribut" status="below">
    <element att1="val1" att="val-avant|val-après" att3="val3" status="modified"/>
  </test>
  <test name="Modification attribut depuis vide" status="below">
    <element att1="val1" att="|val" att3="val3" status="modified"/>
  </test>
  <test name="Modification attribut vers vide" status="below">
    <element att1="val1" att="val|" att3="val3" status="modified"/>
  </test>
  <test name="Modification d'un attribut identifiant" status="below">
    <element id="après" status="added"/>
    <element id="avant" status="removed"/>
  </test>
</tests>
